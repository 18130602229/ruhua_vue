.j {
  font-size: 14px;
}
.j .head {
    background: url(data:image/jpeg;base64,) no-repeat;
    background-size: 100% 100%;
    height: 250px;
    width: 100%;
    color: #E8B374;
    box-sizing: border-box;
    padding: 55px 20px 0;
}
.j .head ._img {
      width: 100%;
      height: 168px;
}
.j .head .head_01 {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-pack: justify;
      -webkit-justify-content: space-between;
              justify-content: space-between;
      font-size: 15px;
      padding: 0 4px 40px 15px;
}
.j .head .head_02 {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
}
.j .head .head_02 .head_02_1 {
        line-height: 40px;
        width: 25%;
        text-align: center;
}
.j .taoc {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-justify-content: space-around;
            justify-content: space-around;
    margin: 25px 10px;
}
.j .taoc .tc_01 {
      border: 1px solid #DCDCDC;
      border-radius: 5px;
      text-align: center;
      width: 28%;
      position: relative;
}
.j .taoc .tc_01 .tc_01_1 {
        border-bottom: 1px solid #F2F2F2;
        padding: 8px 0;
        color: #96713F;
}
.j .taoc .tc_01 .tc_01_2 {
        padding: 15px 0;
        color: #7F5214;
}
.j .taoc .tc_01 .tc_01_2 ._span {
          font-size: 24px;
          font-weight: 600;
}
.j .taoc .tc_01 .zhe {
        position: absolute;
        top: -20px;
        left: 50px;
        background: -webkit-linear-gradient(left, #E22208, #FF9E6A);
        background: linear-gradient(to right, #E22208, #FF9E6A);
        color: #fff;
        padding: 3px 8px;
        border-top-left-radius: 15px;
        border-bottom-right-radius: 15px;
        white-space: nowrap;
        font-size: 12px;
}
.j .taoc .zhong {
      border: 1px solid #FFC476;
      border-radius: 5px;
      text-align: center;
      width: 28%;
      position: relative;
}
.j .taoc .zhong .tc_01_1 {
        background-color: #FFCB85;
        border-bottom: 1px solid #FFCB85;
        padding: 8px 0;
        color: #96713F;
}
.j .taoc .zhong .tc_01_2 {
        background-color: #FFDCAD;
        padding: 15px 0;
        color: #7F5214;
}
.j .taoc .zhong .tc_01_2 ._span {
          font-size: 24px;
          font-weight: 600;
}
.j .taoc .zhong .zhe {
        position: absolute;
        top: -20px;
        left: 50px;
        background: -webkit-linear-gradient(left, #E22208, #FF9E6A);
        background: linear-gradient(to right, #E22208, #FF9E6A);
        color: #fff;
        padding: 3px 8px;
        border-top-left-radius: 15px;
        border-bottom-right-radius: 15px;
        white-space: nowrap;
        font-size: 12px;
}
.j .btn {
    margin: 20px 25px;
    background: -webkit-linear-gradient(top, #E8BE86, #D88D28);
    background: linear-gradient(to bottom, #E8BE86, #D88D28);
    color: #fff;
    text-align: center;
    height: 45px;
    line-height: 45px;
    border-radius: 30px;
    font-size: 16px;
    border: 1px solid #EBB163;
    margin-bottom: 40px;
}
.j .quan {
    margin: 10px;
}
.j .quan .q_tit {
      border-left: 2px solid #E09A39;
      padding-left: 10px;
      font-size: 15px;
      font-weight: 600;
      margin-bottom: 10px;
}
.j .quan .q_qy {
      padding: 10px;
}
.j .quan .q_qy ._span {
        padding-left: 10px;
}

